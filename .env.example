# Airtable Configuration Example
# Copy this file to .env and fill in your actual values

AIRTABLE_PERSONAL_ACCESS_TOKEN=your_token_here

# Contact Base - Used for authentication/login
# Contains tables: Students, Parents, Mentors, Writing Coaches, Team
CONTACT_BASE_ID=your_contact_base_id_here

# Invoicing Base - Used for classes and invoicing
# Contains table: Classes
INVOICING_BASE_ID=your_invoicing_base_id_here

# Server Configuration
PORT=3000
